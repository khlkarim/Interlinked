{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "DOSO",
    "description": "Peer to peer communication",
    "action": {
        "default_popup": "./index.html"
    },
    "background": {
        "service_worker": "workers/reloader.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting",
        "nativeMessaging"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
